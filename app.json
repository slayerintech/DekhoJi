{
  "expo": {
    "name": "DekhoJi",
    "slug": "DekhoJi",
    "scheme": "dekhoji",
    "version": "1.0.14",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0a"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#0a0a0a"
      },
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "package": "com.slayer.dekhoji",
      "versionCode": 22,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "VIBRATE",
        "INTERNET",
        "WAKE_LOCK",
        "ACCESS_NETWORK_STATE",
        "MODIFY_AUDIO_SETTINGS",
        "com.google.android.gms.permission.AD_ID"
      ]
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-web-browser",
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24,
            "kotlinVersion": "2.1.20",
            "ndkVersion": "26.1.10909125",
            "usesCleartextTraffic": true,
            "buildToolsVersion": "35.0.0"
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-7503400330650109~6945543023",
        "iosAppId": "ca-app-pub-7503400330650109~6945543023"
        }
      ],
      "./plugins/withAndroidNdkFix"
    ],
    "extra": {
      "eas": {
        "projectId": "1738746f-d71d-4071-97b7-680c2f1f5010"
      }
    }
  }
}
